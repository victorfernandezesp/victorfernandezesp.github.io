<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema%22%3E
    <xs:element name="paises">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="pais" minOccurs="1" maxOccurs="unbounded">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name = "nombre" type="xs:string"/>
                            <xs:element name = "ciudad" type="xs:string" minOccurs="0" maxOccurs="5"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>

    </xs:element>
</xs:schema>